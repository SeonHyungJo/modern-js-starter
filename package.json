{
  "name": "modern-js-starter",
  "version": "1.0.0",
  "description": "Modern JS Project Starter for beginner",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint **/*.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeonHyungJo/modern-js-starter.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeonHyungJo/modern-js-starter/issues"
  },
  "homepage": "https://github.com/SeonHyungJo/modern-js-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-terser": "^4.0.2"
  },
  "peerDependencies": {
    "react": ">=16.2.0"
  },
  "engines": {
    "node": ">=8"
  }
}
